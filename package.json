{
  "name": "ide-reason",
  "main": "./lib/main",
  "private": true,
  "version": "0.1.2",
  "description": "ReasonML/OCaml language support for Atom-IDE.",
  "author": {
    "name": "zaaack",
    "email": "zaaack@qq.com",
    "url": "https://github.com/zaaack"
  },
  "homepage": "https://github.com/zaaack/atom-ide-reason",
  "keywords": [
    "ide",
    "reason",
    "reasonml",
    "ocaml",
    "bucklescript",
    "language-server-protocol"
  ],
  "activationCommands": {},
  "repository": "https://github.com/zaaack/atom-ide-reason",
  "bugs": {
    "url": "https://github.com/zaaack/atom-ide-reason/issues"
  },
  "license": "MIT",
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "dependencies": {
    "atom-languageclient": "^0.1.2",
    "ocaml-language-server": "https://github.com/zaaack/ocaml-language-server.git#3a477e64253b5d05224b776f18c0bb512496d53c"
  },
  "consumedServices": {
    "linter-indie": {
      "versions": {
        "2.0.0": "consumeLinterV2"
      }
    },
    "nuclide-datatip.provider": {
      "versions": {
        "0.0.0": "consumeDatatip"
      }
    }
  },
  "providedServices": {
    "autocomplete.provider": {
      "versions": {
        "2.0.0": "provideAutocomplete"
      }
    },
    "nuclide-code-format.provider": {
      "versions": {
        "0.0.0": "provideCodeFormat"
      }
    },
    "nuclide-definition-provider": {
      "versions": {
        "0.0.0": "provideDefinitions"
      }
    },
    "nuclide-find-references.provider": {
      "versions": {
        "0.0.0": "provideFindReferences"
      }
    },
    "nuclide-outline-view": {
      "versions": {
        "0.0.0": "provideOutlines"
      }
    }
  }
}
